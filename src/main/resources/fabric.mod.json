{
  "schemaVersion": 1,
  "id": "oauth-fabric",
  "version": "${version}",
  "name": "Oauth",
  "description": "Adds the ability to relogin without closing your game. It includes legacy username logins, mojang logins, and microsoft account logins.",
  "authors": [
    "Sintinium"
  ],
  "contact": {},
  "license": "GPL-3.0",
  "icon": "assets/oauth-fabric/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.sintinium.oauthfabric.OAuth"
    ]
  },
  "mixins": [
    "oauth-fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.13.3",
    "minecraft": "1.18.x"
  }
}
